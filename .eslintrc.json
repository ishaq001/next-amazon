{
  "extends": [
    "next/core-web-vitals",
    "eslint:recommended",
    "prettier",
    "next",
    "eslint:recommended"
  ],
  "env": {
    "browser": true
  },
  "rules": {
    "no-unused-vars": "error",
    "semi": ["error", "never"],
    "eqeqeq": ["warn"],
    "quotes": ["error", "single"],
    "no-cond-assign": ["error", "always"]
  }
}
